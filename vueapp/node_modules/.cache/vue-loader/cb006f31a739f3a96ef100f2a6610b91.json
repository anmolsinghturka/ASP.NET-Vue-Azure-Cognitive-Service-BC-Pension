div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"col-md-5 pr-md-1\">\n        <base-input\n          label=\"PERSON ID\"\n          placeholder=\"Person ID\"\n          v-model=\"persionId\"\n        >\n        </base-input>\n      </div>\n\n      <div class=\"col-md-3 px-md-1\">\n        <base-input\n          label=\"FIRST NAME\"\n          placeholder=\"Username\"\n          v-model=\"firstName\"\n        >\n        </base-input>\n      </div>\n\n      <div class=\"col-md-4 pl-md-1\">\n        <base-input\n          label=\"LAST NAME\"\n          type=\"email\"\n          v-model=\"lastName\"\n        >\n        </base-input>\n      </div>\n    </div>\n    \n    <div class=\"row\">\n      <div class=\"col-md-6 pr-md-1\">\n        <base-input\n          label=\"Email\"\n          v-model=\"EMAIL\"\n          placeholder=\"First Name\"\n        >\n        </base-input>\n      </div>\n      <div class=\"col-md-6 pl-md-1\">\n        <base-input\n          label=\"Account No.\"\n          v-model=\"accountNo\"\n          placeholder=\"Last Name\"\n        >\n        </base-input>\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-md-12\">\n        <base-input\n          label=\"Address\"\n          v-model=\"address\"\n          placeholder=\"Home Address\"\n        >\n        </base-input>\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-md-4 pr-md-1\">\n        <base-input label=\"City\" v-model=\"model.city\" placeholder=\"City\">\n        </base-input>\n      </div>\n      <div class=\"col-md-4 px-md-1\">\n        <base-input\n          label=\"Home Phone\"\n          v-model=\"homePhone\"\n          placeholder=\"Country\"\n        >\n        </base-input>\n      </div>\n      <div class=\"col-md-4 pl-md-1\">\n        <base-input \n          label=\"Postal Code\" \n          placeholder=\"Postal Code\"\n          v-model=\"postalcode\"\n        > \n        </base-input>\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-md-8\">\n        <base-input>\n          <label>About Me</label>\n          <textarea\n            rows=\"4\"\n            cols=\"80\"\n            class=\"form-control\"\n            placeholder=\"Here can be your description\"\n            v-model=\"initials\"\n          >\n          </textarea>\n        </base-input>\n      </div>\n    </div>\n    <base-button slot=\"footer\" type=\"primary\" fill>Save</base-button>\n  </card>\n</template>\n<script>\n    import { computed } from 'vue'\n    import { usePersonStore } from '@/stores/person'\n\n    export default {\n        props: {\n            model: {\n                type: Object,\n                default: () => ({})\n            }\n        },\n        setup(props) {\n            const personStore = usePersonStore()\n\n            const fields = [\n                'personId',\n                'firstName',\n                'lastName',\n                'email',\n                'address',\n                'city',\n                'postalcode',\n                'accountNo',\n                'initials',\n                'homePhone'\n            ]\n\n            const computedProps = {}\n\n            fields.forEach((field) => {\n                computedProps[field] = computed({\n                    get: () => store[field],\n                    set: (value) => {\n                        store[field] = value\n                        props.model[field] = value\n                    }\n                })\n            })\n\n            return computedProps\n        }\n    }\n</script>\n<style></style>\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n<!-- eslint-disable vue/no-mutating-props -->\n<template>\n    <card>\n        <div class=\"row\">\n            <div class=\"col-md-5 pr-md-1\">\n                <base-input label=\"CITY\" placeholder=\"CITY\" v-model=\"CITY\">\n                </base-input>\n        </div>\n        </div>\n\n        <div class=\"row\">\n            <div class=\"col-md-6 pr-md-1\">\n                <base-input label=\"Email\" v-model=\"EMAIL\" placeholder=\"First Name\">\n                </base-input>\n        </div>\n        </div>\n\n        <div class=\"row\">\n            <div class=\"col-md-5 pr-md-1\">\n                <base-input label=\"INITIALS\" placeholder=\"INITIALS\" v-model=\"INITIALS\">\n                </base-input>\n            </div>\n        </div>\n  \n\n\n     \n        <base-button slot=\"footer\" type=\"primary\" fill>Save</base-button>\n    </card>\n</template>\n<script>\n    import { computed } from 'vue'\n    import { usePersonStore } from '@/stores/person'\n\n    export default {\n        props: {\n            model: {\n                type: Object,\n                default: () => ({})\n            }\n        },\n        setup() {\n            const personStore = usePersonStore();\n            const EMAIL = computed({\n                get: () => personStore.EMAIL,\n                set: (value) => personStore.EMAIL = value\n            });\n            const INITIALS = computed({\n                get: () => personStore.INITIALS,\n                set: (value) => personStore.INITIALS = value\n            });\n            const CITY = computed({\n                get: () => personStore.CITY,\n                set: (value) => personStore.CITY = value\n            });\n\n            return {\n                EMAIL,\n                INITIALS,\n                CITY\n            }\n        }\n    }\n</script>"]}]}